#pragma once
#include <iostream>

class GameState
{
 private:
    int m_stateIndex;
 public:
    
    GameState(int startingIndex = -1) {
        stateIndex = startingIndex;
    }

    void RunState(void) {
        if (m_stateIndex < 0) {
            std::cerr << "ERROR: GameState does not have a state" << std::endl;
        }
    }
};
